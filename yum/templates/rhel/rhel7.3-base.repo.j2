[rhel-{{ ansible_distribution_major_version }}-server-rpms-pes]
name = Red Hat Enterprise Linux {{ ansible_distribution_major_version }} Server (RPMs)
baseurl = http://{{ repo }}/rhel/{{ ansible_distribution_version }}/iso/
enabled = 1
gpgcheck = 1
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-redhat-release
metadata_expire = 86400
ui_repoid_vars = releasever basearch
